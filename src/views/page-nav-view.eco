<div class="page-nav-wrapper">
	<div class="page-nav" >
		<ul>
			<% for i, link of @links: %>
				<li>
					<a class="ajaxy" href="<%= link.url %>" data-id="<%= link.id %>">
						<span class="black-bar"></span>
						<h4><%= ('0' + (parseInt(i) + 1)).slice(-2) %></h4>
						<h4><%= link.title %></h4>
					</a>
				</li>
			<% end %>
		</ul>
	</div>
	<div class="menu-btn">
		<span></span>
		<span></span>
		<span></span>
	</div>
</div>